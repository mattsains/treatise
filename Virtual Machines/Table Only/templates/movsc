    lodsw
    lea rax, [rsi + rax - 4]
    mov rdi, [rax] ;rdi holds size of string
    add rdi, 8
    push rsi
    push rdi
    push rax
    call malloc
    mov [registers+{i}*8], rax
    mov rdi, rax ;1st argument is destination
    pop rsi ;2nd argument is source
    pop rdx ;3rd arg for memcpy is size
    call memcpy
    pop rsi
    mov rax, [registers+{i}*8]
    mov rbx, [rax]
    shl rbx, 4
    mov [rax], rbx
    xor rax, rax