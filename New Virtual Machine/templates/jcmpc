    lodsw
    mov {s[0]}, [{r['pc']} + rax - 2]
    cmp {r[i]}, {s[0]}
    jl .jmp ; lt pointer is under rsi so use it
    je .eq ;eq is 2B further
    add {r['pc']}, 2 ;otherwise gt, 4B further
    .eq:
    add {r['pc']}, 2
    .jmp:
    lodsw
    movsx {s[0]}, ax
    sub {s[0]}, 2
    add {r['pc']}, {s[0]}